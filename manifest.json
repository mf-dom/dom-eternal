{
    "name": "DOM Eternal",
    "description": "An XSS fuzzer capable of detecting runtime XSS vulnerabilites.",
    "version": "1.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_page": "./options.html",
    "devtools_page": "devtools-src/devtools.html",
    "browser_action": {
        "default_popup": "popup-src/popup.html"
    },
    "permissions": [
        "debugger",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "assets/code-slash-16-16.png",
        "32": "assets/code-slash-32-32.png",
        "48": "assets/code-slash-48-48.png",
        "128": "assets/code-slash-128-128.png"
    }
}
